generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Measure {
  uuid          String   @id @default(uuid()) @db.Uuid
  customer_code String
  datetime      DateTime
  image_url     String
  value         Float
  type          String
}
